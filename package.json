{
  "name": "repentanceruntracker",
  "author": "Aymeric Ingargiola",
  "description": "App to track The Binding of Isaac game's run",
  "version": "0.6.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aymericingargiola/repentance-run-tracker.git"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:inspect": "./node_modules/.bin/electron --inspect=5858 dist_electron",
    "electron:deploy": "vue-cli-service electron:build \"-p\" \"always\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@vuex-orm/core": "^1.0.0-draft.16",
    "adm-zip": "^0.5.6",
    "compare-versions": "^3.6.0",
    "core-js": "^3.6.5",
    "electron-log": "^4.4.0",
    "electron-updater": "^4.3.8",
    "jsonfile": "^6.1.0",
    "luxon": "^2.1.1",
    "v-calendar": "^2.3.4",
    "vue": "^2.6.11",
    "vue-color-gradient-picker": "^0.1.2",
    "vue-router": "^3.5.1",
    "vue2-timepicker": "^1.1.6",
    "vuescroll": "^4.17.3",
    "vuex": "^3.6.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "electron": "^16.0.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "resolutions": {
    "ansi-html": "https://registry.npmjs.org/ansi-html-community/-/ansi-html-community-0.0.8.tgz"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
